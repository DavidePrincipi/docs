#: ../../installation.rst:13
msgid "Minimum requirements"
msgstr "Requerimientos mínimos"

#: ../../installation.rst:15
msgid "Minimum requirements are:"
msgstr "Los requisitos mínimos son:"

#: ../../installation.rst:17
msgid "64 bit CPU (x86_64)"
msgstr "64 bit CPU (x86_64)"

#: ../../installation.rst:18
msgid "1 GB of RAM"
